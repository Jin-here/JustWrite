<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gaw.chat.bean.User" table="t_user">
        <id name="id" column="id" length="36">
            <generator class="uuid2"></generator>
        </id>
        <property name="name" column="name" length="16"></property>
        <property name="password" column="password" length="16"></property>
    </class>
</hibernate-mapping>