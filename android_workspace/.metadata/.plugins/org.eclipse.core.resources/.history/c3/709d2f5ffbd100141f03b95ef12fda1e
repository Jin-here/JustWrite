package test;

public class Test {

	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(Thread.currentThread().getId());
		new Thread(new Runnable() {
			public void run() {
				System.out.println(Thread.currentThread().getId());
				
			}
		}).start();
		try {
			Thread.sleep(1000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println(Thread.currentThread().getId()+"@@");
		
	}

}
